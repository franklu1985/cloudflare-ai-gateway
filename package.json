{
  "name": "galatea-ai-api",
  "version": "1.0.0",
  "description": "A unified AI model gateway built on Cloudflare Workers, providing OpenAI-compatible API interfaces with support for multiple AI providers.",
  "main": "src/index.ts",
  "homepage": "https://github.com/your-username/galatea-ai-api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/galatea-ai-api.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/galatea-ai-api/issues"
  },
  "scripts": {
    "dev": "wrangler dev --env dev",
    "preview": "wrangler dev --env preview",
    "build": "wrangler deploy --dry-run --env preview",
    "deploy": "wrangler deploy --env production",
    "deploy:dev": "wrangler deploy --env dev",
    "deploy:preview": "wrangler deploy --env preview",
    "test": "echo \"No tests available\""
  },
  "keywords": [
    "cloudflare",
    "workers",
    "openai",
    "ai",
    "gateway",
    "typescript",
    "api",
    "proxy",
    "llm",
    "chat",
    "artificial-intelligence",
    "edge-computing"
  ],
  "author": "Galatea AI Gateway Contributors",
