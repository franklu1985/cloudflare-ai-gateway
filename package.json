  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "src/**/*",
    "*.example",
    "README*.md",
    "LICENSE",
    "CONTRIBUTING.md"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/node": "^22.9.0",
    "dotenv": "^17.0.0",
    "eslint": "^9.15.0",
    "typescript": "^5.7.2",
    "wrangler": "^4.21.2"
  },
  "dependencies": {
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "^3.3.2"
  }
